    <select id="selectListView"
		resultType="com.cl.entity.view.WupinchukuView" >

		SELECT  wupinchuku.* FROM wupinchuku  wupinchuku 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.WupinchukuView" >
		SELECT * FROM wupinchuku  wupinchuku <where> 1=1 ${ew.sqlSegment}</where>
	</select>



</mapper>
